<body style="background-color:lightblue;font-family:arial;text-align:center">
  <%= link_to "Pets Index", "/pets" %>
  <%= link_to "Shelters Index", "/shelters" %>
    <br/>
    <br/>
    <br/>
  <%= form_tag '/shelters' do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %>
    <br/>
  <%= label_tag :address %>
  <%= text_field_tag :address %>
    <br/>
  <%= label_tag :city %>
  <%= text_field_tag :city %>
    <br/>
  <%= label_tag :state %>
  <%= text_field_tag :state %>
    <br/>
  <%= label_tag :zip %>
  <%= text_field_tag :zip %>
    <br/>
    <br/>
  <%= submit_tag 'Create Shelter' %>
    <br/>
    <br/>
  <%= link_to "Back to Shelters", "/shelters" %>
<% end %>
